<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="201dp"
        android:layout_height="57dp"
        android:layout_marginTop="28dp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.519"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/strong_agree_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="163dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/nunito_extralight"
        android:text="@string/strongly_agree"
        android:textFontWeight="1000"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/question1"
        app:layout_constraintEnd_toStartOf="@+id/strong_disagree_1"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/question1"
        android:layout_width="310dp"
        android:layout_height="65dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="36dp"
        android:fontFamily="@font/nunito_extralight"
        android:text="@string/i_believe_that_having_scheduled_activities_such_as_meditation"
        android:textFontWeight="1000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/strong_disagree_1" />

    <TableLayout
        android:id="@+id/second_checkboxes"
        android:layout_width="304dp"
        android:layout_height="53dp"
        android:layout_marginStart="39dp"
        android:layout_marginEnd="39dp"
        app:layout_constraintBottom_toTopOf="@+id/strong_agree_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/question1"
        app:layout_constraintVertical_bias="1.0">

        <TableRow android:layout_width="wrap_content">

            <CheckBox
                android:id="@+id/checkBox1_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="30dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />

            <CheckBox
                android:id="@+id/checkBox1_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="30dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />

            <CheckBox
                android:id="@+id/checkBox1_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="30dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />

            <CheckBox
                android:id="@+id/checkBox1_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="30dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />

            <CheckBox
                android:id="@+id/checkBox1_5"
                android:layout_width="63dp"
                android:layout_height="match_parent"
                android:paddingRight="0dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />
        </TableRow>

    </TableLayout>

    <TextView
        android:id="@+id/strong_agree_2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/nunito_extralight"
        android:text="@string/strongly_agree"
        android:textFontWeight="1000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/question1" />

    <TextView
        android:id="@+id/strong_disagree_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="256dp"
        android:layout_marginEnd="16dp"
        android:text="@string/strongly_disagree"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/strong_agree_1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/strong_disagree_2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="28dp"
        android:text="@string/strongly_disagree"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/second_checkboxes" />

    <TableLayout
        android:id="@+id/first_checkboxes"
        android:layout_width="300dp"
        android:layout_height="53dp"
        android:layout_marginStart="39dp"
        android:layout_marginEnd="39dp"
        app:layout_constraintBottom_toTopOf="@+id/strong_agree_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/question2"
        app:layout_constraintVertical_bias="1.0">

        <TableRow android:layout_width="wrap_content">

            <CheckBox
                android:id="@+id/checkBox2_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="30dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />

            <CheckBox
                android:id="@+id/checkBox2_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="30dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />

            <CheckBox
                android:id="@+id/checkBox2_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="30dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />

            <CheckBox
                android:id="@+id/checkBox2_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="30dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />

            <CheckBox
                android:id="@+id/checkBox2_5"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:paddingRight="0dp"
                android:text=""
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="305dp" />
        </TableRow>

    </TableLayout>

    <TextView
        android:id="@+id/question2"
        android:layout_width="310dp"
        android:layout_height="45dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="36dp"
        android:fontFamily="@font/nunito_extralight"
        android:text="@string/i_am_interested_in_socializing"
        android:textFontWeight="1000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="310dp"
        android:layout_height="51dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="125dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="123dp"
        android:fontFamily="@font/nunito_extralight"
        android:gravity="center_vertical"
        android:text="@string/custom_experience_questionnaire"
        android:textFontWeight="1000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/strong_disagree_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.923" />


</androidx.constraintlayout.widget.ConstraintLayout>