<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.Toolbar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/toolbar2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        >

    </androidx.appcompat.widget.Toolbar>

    <View
        android:layout_width="500dp"
        android:layout_height="900dp"
        android:background="@drawable/blue_backgroung_squiggle"
        android:rotation="-12"
        android:rotationY="01"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="600dp"
        android:layout_height="1200dp"
        android:background="@drawable/red_background_squiggle"
        android:rotation="-20"
        android:rotationX="0"
        android:rotationY="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginTop="50dp"
        android:id="@+id/challenges_icon"
        android:background="@drawable/ic_slack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/meditation_pic"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_feather"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="@id/meditation_button"
        app:layout_constraintEnd_toEndOf="@id/meditation_button"
        app:layout_constraintStart_toStartOf="@id/meditation_button"
        app:layout_constraintTop_toTopOf="@id/meditation_button" />

    <View
        android:id="@+id/reading_pic"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_book_open"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="@id/reading_button"
        app:layout_constraintEnd_toEndOf="@id/reading_button"
        app:layout_constraintStart_toStartOf="@id/reading_button"
        app:layout_constraintTop_toTopOf="@id/reading_button" />


    <View
        android:id="@+id/cooking_pic"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_coffee"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="@id/cooking_button"
        app:layout_constraintEnd_toEndOf="@id/cooking_button"
        app:layout_constraintStart_toStartOf="@id/cooking_button"
        app:layout_constraintTop_toTopOf="@id/cooking_button" />

    <View
        android:id="@+id/schedule_pic"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_archive"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="@id/ScheduleButton"
        app:layout_constraintEnd_toEndOf="@id/ScheduleButton"
        app:layout_constraintStart_toStartOf="@id/ScheduleButton"
        app:layout_constraintTop_toTopOf="@id/ScheduleButton" />

    <View
        android:id="@+id/see_all_pic"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/ic_align_center"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="@id/SeeAllButton"
        app:layout_constraintEnd_toEndOf="@id/SeeAllButton"
        app:layout_constraintStart_toStartOf="@id/SeeAllButton"
        app:layout_constraintTop_toTopOf="@id/SeeAllButton" />

    <View
        android:id="@+id/exercise_pic"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/ic_exercise"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="@id/exercise_button"
        app:layout_constraintEnd_toEndOf="@id/exercise_button"
        app:layout_constraintStart_toStartOf="@id/exercise_button"
        app:layout_constraintTop_toTopOf="@id/exercise_button" />
    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/nova_square"
        android:text="Challenges"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/challenges_icon" />

    <Button
        android:id="@+id/exercise_button"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginStart="30dp"
        android:backgroundTint="#ffffff"
        android:text="Button"
        app:cornerRadius="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView34"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/meditation_button"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginStart="80dp"
        android:backgroundTint="#ffffff"
        android:text="Button"
        app:cornerRadius="40dp"
        app:layout_constraintStart_toEndOf="@id/exercise_button"
        app:layout_constraintTop_toBottomOf="@id/textView34"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/reading_button"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginStart="30dp"
        android:backgroundTint="#ffffff"
        android:text="Button"
        app:cornerRadius="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/exercise_button"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/cooking_button"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginStart="80dp"
        android:backgroundTint="#ffffff"
        android:text="Button"
        app:cornerRadius="40dp"
        app:layout_constraintStart_toEndOf="@id/reading_button"
        app:layout_constraintTop_toBottomOf="@id/exercise_button"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/ScheduleButton"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginStart="30dp"
        android:backgroundTint="#ffffff"
        android:text="Button"
        app:cornerRadius="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/reading_button"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/SeeAllButton"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginStart="80dp"
        android:backgroundTint="#ffffff"
        android:text="Button"
        app:cornerRadius="40dp"
        app:layout_constraintStart_toEndOf="@id/ScheduleButton"
        app:layout_constraintTop_toBottomOf="@id/reading_button"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />

</androidx.constraintlayout.widget.ConstraintLayout>